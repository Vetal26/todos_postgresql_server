  },
  "homepage": "https://github.com/Vetal26/todos_postgresql_server#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "pg": "^8.6.0",
    "sequelize": "^6.6.4"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.30.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.23.4",
    "mocha": "^9.0.2",
    "nodemon": "^2.0.9",
    "sequelize-cli": "^6.2.0",
    "sinon": "^11.1.1"
  }
}
