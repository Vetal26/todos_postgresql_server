{
  "name": "todos_postgresql_server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:dev": "nodemon -r dotenv/config index.js",
    "db:create": "sequelize-cli db:create",
    "db:migrate": "sequelize-cli db:migrate",
    "db:g:migration": "sequelize-cli migration:generate --name",
    "db:g:seed": "sequelize-cli seed:generate --name",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:create:test": "sequelize-cli --env test db:create",
    "db:migrate:test": "sequelize-cli --env test db:migrate",
    "db:seeds:test": "sequelize-cli --env test db:seed:all",
    "test": "cross-env NODE_ENV=test mocha --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vetal26/todos_postgresql_server.git"
  },
